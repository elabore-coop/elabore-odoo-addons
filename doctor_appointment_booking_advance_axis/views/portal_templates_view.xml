<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

    <template id="portal_my_home_menu_helpdesk" name="Portal layout : helpdesk tickets menu entries" inherit_id="portal.portal_my_home" priority="50">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-if="appointment_count" t-call="portal.portal_docs_entry">
                <t t-set="title">Appointments</t>
                <t t-set="url" t-value="'/my/appointments'"/>
                <t t-set="count" t-value="appointment_count"/>
            </t>
        </xpath>
        </template>
    </data>
</odoo>